<html>



 
<h2>Listing Profiles</h2></br>



                    
    
    
    <table class="table table-striped table-hover">
    
      <thead class="thead-dark">
        <tr>
        <th>Profile Name</th>
        <th>Profile ID </th>
        <th>Base_url</th>
        <th>Auth_type</th>
        <th>Edit</th>
        <th>Delete</th>
        
        </tr>
        </thead>
        </div>
  <tbody>
  <% @profiles.each do |profile| %>
    <tr>
      <td><%= profile.name %></td>
      <td><%= profile.id %></td>
      <td><%= profile.base_url %></td>
      <td><%= profile.auth_type %></td>
      
      <td class="actions">
              <%= link_to edit_profile_path(profile),
                          title: 'Edit profile',
                          class: 'button tiny edit' do %>
                <i class="fa fa-edit"></i>
              <% end %>
      <td><%= link_to profile_path(profile), 
               method: :delete,
               data: { confirm: 'Are you sure?' } ,class: 'button tiny delete' do%>
          <i class="fa fa-remove"></i>
          <% end %>
    </tr>
    </tbody>
   </span>
  <% end %>
</table>
</span>
</div>


</div>

 

   

</html>
